
#ifndef GUARD_str_link_h
#define GUARD_str_link_h

#include<vector>
#include<string>

using std::vector;
using std::string;

vector<string> link(const vector<string>& top,const vector<string>& below)
{
  vector<string> ret=top;

  vector<string>::size_type i=0;

  while(i!=below.size()){
    ret.push_back(below[i]);
  i++;
  }
return ret;
  
}
#endif
